{
  "name": "aiwm-worker",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "services/aiwm-worker/src",
  "projectType": "application",
  "tags": ["type:app", "scope:worker"],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/services/aiwm-worker",
        "main": "services/aiwm-worker/src/main.ts",
        "tsConfig": "services/aiwm-worker/tsconfig.app.json",
        "assets": ["services/aiwm-worker/src/assets"],
        "webpackConfig": "services/aiwm-worker/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "aiwm-worker:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "aiwm-worker:build:development"
        },
        "production": {
          "buildTarget": "aiwm-worker:build:production"
        }
      }
    }
  }
}
